PRIMARY.ENV=local

SERVER.PORT=8080
SERVER.READ_TIMEOUT=30
SERVER.WRITE_TIMEOUT=30
SERVER.IDLE_TIMEOUT=60
SERVER.CORS_ALLOWED_ORIGINS=http://localhost:8000
SERVER.JWT_KEY=secret_key

DATABASE.TYPE=postgres
DATABASE.HOST=postgres
DATABASE.PORT=5432
DATABASE.USER=postgres
DATABASE.PASSWORD=password
DATABASE.NAME=postgres
DATABASE.SSL_MODE=disable
DATABASE.MAX_OPEN_CONNS=10
DATABASE.MAX_IDLE_CONNS=10
DATABASE.CONN_MAX_LIFETIME=300
DATABASE.CONN_MAX_IDLE_TIME=60

AI_MANAGER.PROVIDER=https://openrouter.ai/api/v1
AI_MANAGER.API_KEY=api_key
LOGGING.LEVEL=info
LOGGING.FORMAT=json

OBSERVABILITY.SERVICE_NAME=axis
OBSERVABILITY.ENVIRONMENT=development

OBSERVABILITY.LOGGING.LEVEL=debug
OBSERVABILITY.LOGGING.FORMAT=console
OBSERVABILITY.LOGGING.SLOW_QUERY_THRESHOLD=100ms

OBSERVABILITY.HEALTH_CHECKS.ENABLED=true
OBSERVABILITY.HEALTH_CHECKS.INTERVAL=30s
OBSERVABILITY.HEALTH_CHECKS.TIMEOUT=5s
OBSERVABILITY.HEALTH_CHECKS.CHECKS=database
